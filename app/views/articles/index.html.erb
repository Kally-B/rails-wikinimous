<%= render "navbar", article: @article %>

<div class="container">
  <div class="col-sm-8 col-sm-offset-2">
    <% @articles.each do |article| %>
      <article>
        <h3><%= link_to article.title, article_path(article) %></h3>
        <p><%= article.content %></p>
      </article>
    <% end %>
  </div>
</div>
